{
    "manifest_version": 3,
    "name": "Localhost Bookmarks",
    "version": "1.0",
    "description": "Manage and access localhost addresses easily",
    "permissions": [
      "storage",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.svg",
        "48": "images/icon48.svg",
        "128": "images/icon128.svg"
      }
    },
    "icons": {
      "16": "images/icon16.svg",
      "48": "images/icon48.svg",
      "128": "images/icon128.svg"
    },
    "background": {
      "service_worker": "background.js"
    }
  }